<!DOCTYPE html>
<html>
<body>
    <form method="post" {{ form_enctype(form) }}>
        {{ form_start(form) }}
        <div>
            {{ form_errors(form.name) }}
            {{ form_label(form.name) }}
            {{ form_widget(form.name) }}
        </div>
        {{ form_row(form.save, { 'label': 'Submit' }) }}
        {{ form_end(form) }}
    </form>
    </body>
</html>
