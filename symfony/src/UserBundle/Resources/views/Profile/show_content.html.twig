{% trans_default_domain 'FOSUserBundle' %}

<div class="user_user_show">
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
    <p>{{ 'profile.show.first_name'|trans }}: {{ user.firstName }}</p>
    <p>{{ 'profile.show.last_name'|trans }}: {{ user.lastName }}</p>
    <p>{{ 'profile.show.gender'|trans }}: {{ user.gender }}</p>
    <p>{{ 'profile.show.birthdate'|trans}}:{% if user.birthdate != null %} {{ user.birthdate|date("m/d/Y") }}</p> {% endif %}
    <p>{{ 'profile.show.daily_fees_min'|trans }}: {{ user.dailyFeesMin }}</p>
    <p>{{ 'profile.show.daily_fees_max'|trans }}: {{ user.dailyFeesMax }}</p>
    <p>{{ 'profile.show.country'|trans }}:{% if user.address != null %} {{ user.address.country|countryName }}</p> {% endif %}
    <p>{{ 'profile.show.state'|trans }}:{% if user.address != null %} {{ user.address.state }}</p> {% endif %}
    <p>{{ 'profile.show.city'|trans }}:{% if user.address != null %} {{ user.address.city }}</p> {% endif %}
    <p>{{ 'profile.show.zipcode'|trans }}:{% if user.address != null %} {{ user.address.zipcode }}</p> {% endif %}
    <p>{{ 'profile.show.address'|trans }}:{% if user.address != null %} {{ user.address.street }}</p> {% endif %}
    <p>{{ 'profile.show.address'|trans }}:{% if user.address != null %} {{ user.address.street2 }}</p> {% endif %}
    <p>{{ 'profile.show.phone'|trans }}: {{ user.phone }}</p>
    <p>{{ 'profile.show.image'|trans }}: {{ user.image }}</p>
</div>
