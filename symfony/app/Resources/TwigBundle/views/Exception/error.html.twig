{% extends 'base.html.twig' %}

{% block body %}
    <div class="wmw-onboard col-xs-10 col-xs-offset-1 col-md-6 col-md-offset-3 col-xl-4 col-xl-offset-4">
        <div class="wmw-onboard-body">
            <h1>{{ status_code }} - {{ status_text }}</h1>
            <p>
            <h3>
                The requested page couldn't be located. Checkout for any URL
                misspelling or

                {% if app.user %}
                    <a href="{{ url('dashboard') }}">return to the dashboard.</a>.
                {% else %}
                    <a href="{{ url('home_page_expert') }}">return to the homepage.</a>.
                {% endif %}
            </h3>
        </div>
    </div>
{% endblock %}